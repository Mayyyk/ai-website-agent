{
  "id": "solution-section",
  "name": "Solution Section",
  "type": "section",
  "version": "1.0.0",
  "header": {
    "type": "header",
    "classes": "fixed top-0 left-0 right-0 h-16 bg-slate-800/95 backdrop-blur-sm z-50 border-b border-white/10",
    "content": {
      "container": {
        "classes": "container mx-auto px-6 h-16 flex items-center justify-between",
        "logo": {
          "text": "Brainpower AI>>",
          "typography": {
            "preset": "logo",
            "tag": "div",
            "classes": "text-[48px] font-bold text-white leading-none"
          }
        },
        "nav": {
          "classes": "flex items-center gap-8",
          "items": [
            {
              "text": "Solutions",
              "typography": {
                "preset": "nav",
                "classes": "text-white/90 hover:text-white transition-colors"
              }
            },
            {
              "text": "Pricing",
              "typography": {
                "preset": "nav",
                "classes": "text-white/90 hover:text-white transition-colors"
              }
            },
            {
              "text": "About",
              "typography": {
                "preset": "nav",
                "classes": "text-white/90 hover:text-white transition-colors"
              }
            }
          ],
          "cta": {
            "text": "Get Started",
            "type": "button",
            "classes": "ml-8 px-6 py-2 bg-blue-500 hover:bg-blue-600 text-white rounded-lg transition-colors"
          }
        }
      }
    }
  }
} 